<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.filestorage.client.CloudPartController">
  
  <HBox>
    <ComboBox />
    <TextField HBox.hgrow="ALWAYS" />
  </HBox>
  
  <TableView fx:id = "fileTable" VBox.vgrow="ALWAYS" />
  
  <HBox>
    <Button text="Upload" onAction="#upload" />
    <padding>
       <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>
  </HBox>
  
</VBox>


